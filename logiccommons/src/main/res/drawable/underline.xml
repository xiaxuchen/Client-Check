<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 连框颜色值 -->
    <item>
        <shape>
            <solid android:color="@color/material_grey_600" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
