<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="#4dd664" />
            <corners android:radius="30dp"/>
            <size android:width="50dp" android:height="26dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <solid android:color="@color/material_grey_300" />
            <corners android:radius="30dp"/>
            <!--低版本手机需要设置size才能正常显示track-->
            <size android:width="50dp" android:height="28dp" />
            <!--设置边框-->
            <stroke android:color="#fff" android:width="1dp" android:dashGap="1dp" android:dashWidth="1dp"/>
        </shape>
    </item>
</selector>
